			"settings": {
				"fontStyle": "bold",
				"foreground": "#00b8ad"
			}
		},
		{
			"name": "TS: typing",
			"scope": [
				"entity.name.type",
			],
			"settings": {
				"fontStyle": "bold underline",
				"foreground": "#5451d6"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
