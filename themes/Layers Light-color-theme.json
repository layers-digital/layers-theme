	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#A3B1BF"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#ed6b4f"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type",
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#00b8ad"
			}
		},
		{
			"name": "New Operator",
			"scope": [
				"keyword.operator.new",
			],
			"settings": {
				"foreground": "#ed6b4f"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#434e5b"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable.parameter"
			],
